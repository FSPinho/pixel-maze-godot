; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="pixel-maze-godot"
run/main_scene="res://scenes/Game.tscn"

[autoload]

Global="*res://global/Global.gd"

[display]

window/size/width=1080
window/size/height=1920
window/size/resizable=false
window/size/test_width=506
window/size/test_height=900
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/physics_fps=120
2d/physics_engine="GodotPhysics"
2d/thread_model=2

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
threads/thread_model=2
environment/default_clear_color=Color( 0.215686, 0.278431, 0.309804, 1 )
quality/depth/hdr=false
